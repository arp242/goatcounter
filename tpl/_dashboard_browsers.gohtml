<div class="hchart" data-detail="/hchart-detail?kind=browser" data-more="/hchart-more?kind=browser">
	<h2>浏览器</h2>
	{{template "_dashboard_warn_collect.gohtml" .IsCollected}}
	{{if .Err}}
		<em>Error: {{.Err}}</em>
	{{else}}
		{{horizontal_chart .Context .Stats .TotalUniqueUTC 6 true true}}
	{{end}}
</div>
